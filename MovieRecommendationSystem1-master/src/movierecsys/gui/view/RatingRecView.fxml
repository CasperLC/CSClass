<AnchorPane id="AnchorPane" fx:id="RatingBorderPane" prefHeight="604.0" prefWidth="472.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="movierecsys.gui.controller.RatingRecViewController">
   <children>
      <Button fx:id="btnXRatingBack" layoutX="429.0" layoutY="14.0" mnemonicParsing="false" onAction="#xRatingBack" prefHeight="0.0" prefWidth="29.0" text="X" />
