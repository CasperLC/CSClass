            <Button fx:id="btnRatingBadOption" mnemonicParsing="false" onAction="#ratingBadOption" prefHeight="22.0" prefWidth="70.0" text="bad" />
            <Button fx:id="btnRatingNeutralOption" mnemonicParsing="false" onAction="#ratingNeutralOption" prefHeight="22.0" prefWidth="70.0" text="neutral" />
            <Button fx:id="btnRatingGoodOption" mnemonicParsing="false" onAction="#ratingGoodOption" prefHeight="22.0" prefWidth="70.0" text="good" />
            <Button fx:id="btnRatingSuperGoodOption" mnemonicParsing="false" onAction="#ratingSuperGoodOption" prefHeight="22.0" prefWidth="70.0" text="SuperGood" />
